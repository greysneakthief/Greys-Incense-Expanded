<Defs>  
  <ThingDef ParentName="ResourceBase">
    <defName>GI_Essential_Oils</defName>
    <label>essential oils</label>
    <description>Essential oils are efficiently extracted organic components of plants, which are often said to contain the essence of the plant.</description>
    <possessionCount>10</possessionCount>
    <graphicData>
      <texPath>Things/Items/Resource/Essential</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
      <drawSize>0.90</drawSize>
    </graphicData>
    <stackLimit>150</stackLimit>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <MarketValue>6</MarketValue>
      <Mass>0.02</Mass>
      <Flammability>0.7</Flammability>
      <DeteriorationRate>1.0</DeteriorationRate>
    </statBases>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>

